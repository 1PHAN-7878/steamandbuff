<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iphan.dao.ProductDatabaseDAO">
    <select id="findLike" parameterType="String" resultType="String">
        SELECT productName
        FROM product
        WHERE productName LIKE CONCAT('%', '${nameLike}', '%')
    </select>
</mapper>